{
    "manifest_version": 2,
    "web_accessible_resources": [
        "*.jpg",
        "*.jpeg",
        "*.png",
        "*.json",
        "*.png/",
        "*.jpg/"
      ],
      "background": {
        "scripts": ["background.js"]
      },
      "permissions": [
        "tabs",
        "storage",
        "notifications",
        "http://*/",
        "https://*/"
      ],
    "name": "Tim's Thoughts",
    "version": "0.1",
    "browser_action": {
        "default_icon": "img/logo.png"
    }, 
    "content_scripts": [
        {
            "matches": [
                 "<all_urls>"
            ],
            "js": ["content.js"],
            "css": ["style.css"]
        }
    ]
 }